{
  "name": "markedit-theme-gruvbox",
  "version": "1.0.0",
  "description": "Gruvbox theme for MarkEdit.",
  "scripts": {
    "build": "vite build",
    "reload": "osascript -e 'quit app \"MarkEdit\"' -e 'delay 1' -e 'launch app \"MarkEdit\"'",
    "uninstall": "rm ~/Library/Containers/app.cyan.markedit/Data/Documents/scripts/$(node -p \"require('./package.json').name\").js"
  },
  "license": "MIT",
  "devDependencies": {
    "@codemirror/language": "^6.0.0",
    "@codemirror/state": "^6.0.0",
    "@codemirror/view": "^6.0.0",
    "@types/node": "^22.0.0",
    "markedit-api": "https://github.com/MarkEdit-app/MarkEdit-api#v0.11.0",
    "markedit-vite": "https://github.com/MarkEdit-app/MarkEdit-vite#v0.3.0",
    "typescript": "^5.0.0",
    "vite": "^7.0.0"
  },
  "dependencies": {
    "cm6-theme-gruvbox-dark": "^0.2.0",
    "cm6-theme-gruvbox-light": "^0.2.0",
    "markedit-theming": "https://github.com/MarkEdit-app/MarkEdit-theming#v0.10.0"
  }
}
